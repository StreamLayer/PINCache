{
  "branch": "master",
  "plugins": [
    ["@semantic-release/commit-analyzer", {
      "preset": "angular"
    }],
    ["@semantic-release/exec", {
      "prepareCmd": "carthage build --commitish v${nextRelease.version} --cache-builds --archive --archive-output PINCache.framework.zip"
    }],
    ["@semantic-release/git", {
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }],
    ["@semantic-release/github", {
      "assets": [
        { "path": "PINCache.framework.zip" }
      ]
    }]
  ]
}
